{
  "name": "@ledgerhq/lumen-design-core",
  "version": "0.0.50",
  "license": "Apache-2.0",
  "keywords": [
    "design-system",
    "tailwind",
    "figma"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./symbols/*": "./symbols/icons/*"
  },
  "files": [
    "dist",
    "symbols",
    "!**/*.tsbuildinfo",
    "!dist/figma/*"
  ],
  "dependencies": {
    "tailwindcss": "^4.1.17",
    "tslib": "^2.3.0"
  }
}
